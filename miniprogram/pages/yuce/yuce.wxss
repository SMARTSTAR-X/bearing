/* pages/yuce/yuce.wxss */
.circleContainer {
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
  height: 140px;
  box-sizing: border-box;
  padding: 10px;
  margin-top: 150px
}

.circle {
  height: 120px;
  width: 120px;
  border-radius: 60%;
  background: #237ab7;
  display: flex;
  justify-content: center;
  align-items: center;
}

.innerCircle {
  height: 110px;
  width: 110px;
  border-radius: 50%;
  background: white;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 20px;
}